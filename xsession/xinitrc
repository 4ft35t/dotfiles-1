#!/bin/sh

setxkbmap -layout us,es -variant altgr-intl, -option grp:menu_toggle -option grp_led:caps -option ctrl:nocaps
xrdb -merge ~/.Xresources
~/.bin/xcape
udiskie &
autocutsel -fork &
autocutsel -selection PRIMARY -fork &
xcompmgr -I1 -O1 -Ff &
urxvtd -q -o -f &
~/.bin/i8kfans.py &
exec awesome
